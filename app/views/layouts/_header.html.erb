<div class="header">
  <div class="header_nav wrapper">
    <% if user_signed_in? %>
      <%= link_to homes_top_path do %>
        <h1 class="header_logo">
          <%= image_tag 'no_image.jpg', class: "header_logo_image" %>
        </h1>
      <% end %>
      <div class="hamburger" id="open_nav">
        <%= image_tag 'hamburger.png', class: "hamburder_icon" %>
      </div>
    <% else %>
      <%= link_to root_path do %>
        <h1 class="header_logo">
          <%= image_tag 'no_image.jpg', class: "header_logo_image" %>
        </h1>
      <% end %>
      <div class="hamburger" id="open_nav">
        <%= image_tag 'hamburger.png', class: "hamburder_icon" %>
      </div>
    <% end %>
  </div>
</div>