<h1>編集画面</h1>

<%= form_with model:@user, url: user_path, local:true do |f|%>
  <h4>ユーザーネーム</h4>
  <%= f.text_field :user_name %>
  <h4>名前</h4>
  <%= f.text_field :last_name %> <%= f.text_field :first_name %>
  <h4>カナ</h4>
  <%= f.text_field :last_name_kana %> <%= f.text_field :first_name_kana %>
  <h4>都道府県</h4>
  <%= f.select :prefectures, User.prefectures.keys, {} %>


  <%= f.submit '訂正' %>
<% end %>

<%= link_to "詳細へ戻る", user_path %>
